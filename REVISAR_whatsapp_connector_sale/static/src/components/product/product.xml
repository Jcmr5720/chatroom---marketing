<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="chatroom.Product" t-inherit="whatsapp_connector.chatroom.Product" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('product_options_btn')]" position="inside">
            <button
                class="btn btn-sm text-primary fa fa-shopping-cart acrux_add_to_order"
                type="button"
                t-on-click="productOption"
                title="Add to Order" />
        </xpath>
    </t>

</templates>
